{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "strict": true,
        "declaration": true,
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist",
        "noImplicitAny": true,
        "noImplicitThis": false,
        "removeComments": false,
        "noEmitHelpers": true,
        "importHelpers": true,
        "baseUrl": ".",
